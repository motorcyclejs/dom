{
  "name": "@motorcycle/dom",
  "version": "2.0.0",
  "description": "Snabbdom driver for MotorcycleJS.",
  "main": "lib/index.js",
  "directories": {
    "lib": "./lib"
  },
  "files": [
    "lib/"
  ],
  "scripts": {
    "eslint": "eslint src/",
    "test-node": "mocha --compilers js:babel-core/register test/node",
    "test-browser": "testem",
    "test": "npm run eslint && npm run test-node && npm run test-browser",
    "test-perf-basic": "testem -f perf/basic/.testem.json",
    "test-perf-dbmon": "testem -f perf/dbmon/.testem.json",
    "test-ci": "npm run eslint && npm run test-node && testem ci",
    "start": "npm install && npm prune && validate-commit-msg",
    "lib": "rimraf lib/ && mkdirp lib && babel -d lib/ src/",
    "prepublish": "npm run lib",
    "release": "npm run release-patch",
    "release-patch": "git checkout master && release patch && npm publish --access=public",
    "release-minor": "git checkout master && release minor && npm publish --access=public",
    "release-major": "git checkout master && release major && npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/motorcyclejs/dom"
  },
  "keywords": [
    "Cyclejs",
    "Cycle",
    "Most",
    "Mostjs",
    "Reactive",
    "Framework"
  ],
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/motorcyclejs/dom/issues"
  },
  "homepage": "https://github.com/motorcyclejs/dom#readme",
  "contributors": [
    "Frederik Krautwald <fkrautwald@gmail.com> (https://github.com/Frikki)"
  ],
  "dependencies": {
    "@most/dom-event": "^1.2.0",
    "@most/hold": "^1.2.0",
    "@motorcycle/core": "^1.3.0",
    "matches-selector": "^1.0.0",
    "most-subject": "^4.0.2",
    "snabbdom": "^0.4.2",
    "snabbdom-selector": "^0.3.2",
    "snabbdom-to-html": "^2.1.3"
  },
  "devDependencies": {
    "@cycle/isolate": "^1.3.3",
    "@most/eslint-config-most": "^1.0.2",
    "@motorcycle/core": "^1.3.0",
    "assert": "^1.3.0",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-plugin-syntax-jsx": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "cli-release": "^1.0.4",
    "eslint": "^2.10.2",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "most": "^0.19.4",
    "rimraf": "^2.5.2",
    "snabbdom-jsx": "^0.3.0",
    "testem": "^1.7.1",
    "validate-commit-message": "^3.0.1"
  }
}
